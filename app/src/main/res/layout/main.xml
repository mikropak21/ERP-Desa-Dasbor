<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">

	<LinearLayout
		android:id="@+id/linear5"
		android:layout_width="match_parent"
		android:layout_height="40dp"
		android:background="#006064"
		android:orientation="horizontal">

		<ImageView
			android:id="@+id/imageview5"
			android:layout_width="25dp"
			android:layout_height="25dp"
			android:layout_gravity="center_vertical"
			android:layout_marginLeft="8dp"
			android:layout_marginTop="2dp"
			android:layout_marginBottom="2dp"
			android:background="@android:color/transparent"
			android:padding="5dp"
			android:scaleType="fitCenter"
			android:src="@drawable/panahki" />

		<TextView
			android:id="@+id/textview5"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_gravity="center_vertical"
			android:padding="8dp"
			android:text="Tambah Komoditas"
			android:textColor="#FFFFFF"
			android:textSize="12sp" />
	</LinearLayout>
	<ScrollView
		android:id="@+id/vscroll2"
		android:layout_width="800dp"
		android:layout_height="480dp"
		android:padding="8dp">
		<LinearLayout
			android:id="@+id/linear11"
			android:layout_width="600dp"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:orientation="vertical">
			<TextView
				android:id="@+id/textview1"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="20dp"
				android:text="Jenis Komoditas"
				android:textSize="10sp"
				android:textColor="#546E7A"/>
			<Spinner
				android:id="@+id/jk"
				android:layout_width="317dp"
				android:layout_height="20dp"
				android:layout_marginLeft="15dp"
				android:spinnerMode="dropdown"/>
			<TextView
				android:id="@+id/textview2"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="20dp"
				android:layout_marginTop="5dp"
				android:text="Komoditas"
				android:textSize="10sp"
				android:textColor="#546E7A"/>
			<Spinner
				android:id="@+id/k"
				android:layout_width="317dp"
				android:layout_height="20dp"
				android:layout_marginLeft="15dp"
				android:spinnerMode="dropdown"/>
			<TextView
				android:id="@+id/textview3"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="20dp"
				android:layout_marginTop="5dp"
				android:text="Luas Lahan"
				android:textSize="10sp"
				android:textColor="#546E7A"/>
			<LinearLayout
				android:id="@+id/linear4"
				android:layout_width="350dp"
				android:layout_height="35dp"
				android:layout_marginLeft="5dp"
				android:background="@android:color/transparent"
				android:orientation="horizontal">
				<EditText
					android:id="@+id/edittext1"
					android:layout_width="225dp"
					android:layout_height="wrap_content"
					android:layout_marginLeft="10dp"
					android:textSize="12sp"
					android:textColor="#000000"
					android:textColorHint="#607D8B"
					android:inputType="numberDecimal"
					android:translationX="1.0dp"
					android:translationY="1.0dp"
					android:layout_gravity="bottom"/>
				<Spinner
					android:id="@+id/ll"
					android:layout_width="82dp"
					android:layout_height="wrap_content"
					android:layout_marginLeft="11dp"
					android:padding="8dp"
					android:translationX="-1.0dp"
					android:translationY="-1.0dp"
					android:spinnerMode="dropdown"
					android:layout_gravity="right"/>
			</LinearLayout>
			<TextView
				android:id="@+id/textview4"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="20dp"
				android:layout_marginTop="5dp"
				android:text="Tanggal Tanam"
				android:textSize="10sp"
				android:textColor="#546E7A"/>
			<EditText
				android:id="@+id/edittext2"
				android:layout_width="300dp"
				android:layout_height="wrap_content"
				android:layout_marginLeft="15dp"
				android:textSize="12sp"
				android:textColor="#000000"
				android:textColorHint="#607D8B"/>
			<TextView
				android:id="@+id/textview6"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="20dp"
				android:layout_marginTop="5dp"
				android:text="Usia Tanaman"
				android:textSize="10sp"
				android:textColor="#546E7A"/>
			<EditText
				android:id="@+id/edittext3"
				android:layout_width="300dp"
				android:layout_height="wrap_content"
				android:layout_marginLeft="15dp"
				android:textSize="12sp"
				android:textColor="#000000"
				android:textColorHint="#607D8B"
				android:inputType="numberSigned"/>
			<TextView
				android:id="@+id/textview7"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="15dp"
				android:layout_marginTop="5dp"
				android:text="Tanggal Siap Panen"
				android:textSize="10sp"
				android:textColor="#546E7A"/>
			<EditText
				android:id="@+id/edittext4"
				android:layout_width="300dp"
				android:layout_height="wrap_content"
				android:layout_marginLeft="15dp"
				android:textSize="12sp"
				android:textColor="#000000"
				android:textColorHint="#607D8B"/>
			<TextView
				android:id="@+id/textview8"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="15dp"
				android:layout_marginTop="5dp"
				android:text="Perkiraan Hasil Panen"
				android:textSize="10sp"
				android:textColor="#546E7A"/>
			<LinearLayout
				android:id="@+id/linear15"
				android:layout_width="330dp"
				android:layout_height="35dp"
				android:layout_marginLeft="5dp"
				android:orientation="horizontal">
				<EditText
					android:id="@+id/edittext10"
					android:layout_width="225dp"
					android:layout_height="wrap_content"
					android:layout_marginLeft="10dp"
					android:gravity="bottom"
					android:textSize="12sp"
					android:textColor="#000000"
					android:textColorHint="#607D8B"
					android:layout_gravity="bottom"/>
				<Spinner
					android:id="@+id/php"
					android:layout_width="82dp"
					android:layout_height="wrap_content"
					android:layout_marginLeft="11dp"
					android:padding="8dp"
					android:spinnerMode="dropdown"/>
			</LinearLayout>
			<TextView
				android:id="@+id/textview9"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="15dp"
				android:layout_marginTop="5dp"
				android:text="Nama Pemilik"
				android:textSize="10sp"
				android:textColor="#546E7A"/>
			<EditText
				android:id="@+id/edittext7"
				android:layout_width="300dp"
				android:layout_height="30dp"
				android:layout_marginLeft="15dp"
				android:padding="8dp"
				android:textSize="12sp"
				android:textColor="#000000"
				android:textColorHint="#607D8B"/>
			<TextView
				android:id="@+id/textview10"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="15dp"
				android:layout_marginTop="5dp"
				android:text="NIK / KK"
				android:textSize="10sp"
				android:textColor="#546E7A"/>
			<LinearLayout
				android:id="@+id/linear12"
				android:layout_width="wrap_content"
				android:layout_height="35dp"
				android:layout_marginLeft="5dp"
				android:orientation="horizontal">
				<EditText
					android:id="@+id/edittext8"
					android:layout_width="225dp"
					android:layout_height="wrap_content"
					android:layout_marginLeft="10dp"
					android:textSize="12sp"
					android:textColor="#000000"
					android:textColorHint="#607D8B"
					android:inputType="numberDecimal"/>
				<Spinner
					android:id="@+id/nikkk"
					android:layout_width="82dp"
					android:layout_height="wrap_content"
					android:layout_marginLeft="11dp"
					android:padding="8dp"
					android:spinnerMode="dropdown"/>
			</LinearLayout>
			<TextView
				android:id="@+id/textview11"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="15dp"
				android:layout_marginTop="5dp"
				android:text="Jumlah Tenaga Kerja"
				android:textSize="10sp"
				android:textColor="#546E7A"/>
			<EditText
				android:id="@+id/edittext9"
				android:layout_width="300dp"
				android:layout_height="wrap_content"
				android:layout_marginLeft="15dp"
				android:textSize="12sp"
				android:textColor="#000000"
				android:textColorHint="#607D8B"
				android:inputType="numberDecimal"/>
			<TextView
				android:id="@+id/textview12"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="15dp"
				android:layout_marginTop="5dp"
				android:text="Lokasi / Wilayah"
				android:textSize="10sp"
				android:textColor="#546E7A"/>
			<LinearLayout
				android:id="@+id/linear16"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/imageview4"
					android:layout_width="80dp"
					android:layout_height="80dp"
					android:layout_marginLeft="7dp"
					android:src="@drawable/gunung"
					android:scaleType="fitCenter"/>
				<Button
					android:id="@+id/button2"
					android:layout_width="200dp"
					android:layout_height="30dp"
					android:layout_marginLeft="15dp"
					android:padding="5dp"
					android:background="#006064"
					android:text="Tetapkan"
					android:textSize="12sp"
					android:textColor="#FFFFFF"
					android:layout_gravity="center_vertical"/>
			</LinearLayout>
			<TextView
				android:id="@+id/textview13"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="15dp"
				android:layout_marginTop="5dp"
				android:text="Foto"
				android:textSize="10sp"
				android:textColor="#546E7A"/>
			<LinearLayout
				android:id="@+id/linear18"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/imageview3"
					android:layout_width="80dp"
					android:layout_height="80dp"
					android:layout_marginLeft="7dp"
					android:src="@drawable/kamera"
					android:scaleType="fitCenter"/>
				<Button
					android:id="@+id/button3"
					android:layout_width="200dp"
					android:layout_height="30dp"
					android:layout_marginLeft="15dp"
					android:padding="8dp"
					android:background="#006064"
					android:text="Ambil / Pilih"
					android:textSize="12sp"
					android:textColor="#FFFFFF"
					android:layout_gravity="center_vertical"/>
			</LinearLayout>
		</LinearLayout>
	</ScrollView>
	<Button
		android:id="@+id/button1"
		android:layout_width="300dp"
		android:layout_height="wrap_content"
		android:layout_marginLeft="30dp"
		android:layout_marginTop="10dp"
		android:background="#006064"
		android:text="Simpan"
		android:textSize="12sp"
		android:textStyle="bold"
		android:textColor="#FFFFFF"/>
</LinearLayout>
